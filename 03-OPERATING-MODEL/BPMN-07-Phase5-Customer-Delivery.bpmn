  <!-- ============================================================ -->
  <!-- PROCESS: Phase 5 - Customer Delivery & Ongoing Support       -->
  <!-- ============================================================ -->
  <process id="Process_Phase5_CustomerDelivery" name="Phase 5: Customer Delivery &amp; Ongoing Support" isExecutable="false">
    <documentation>
      Luminous BioSolutions NA Monitoring Service
      Phase 5: Customer Delivery &amp; Ongoing Support - Detailed Process

      This process covers customer notification, reporting, and ongoing support.
      Owner: Luminous Customer Success
      Location: Remote (with periodic on-site visits)

      Reference: [[01-Service-Delivery-Process-Map]] - Phase 5
      Master Diagram: [[BPMN-01-Master-Orchestration]]
      Previous Phase: [[BPMN-06-Phase4-Platform-Processing]]
      Loops back to: [[BPMN-03-Phase1-Sample-Collection]] (next sampling cycle)

      Related: [[02-Roles-and-Responsibilities]] | [[05-Pilot-Deliverables-Framework]]

      Version 1.1 | Created: 2026-01-01
    </documentation>

    <!-- Start Event -->
    <startEvent id="Event_Start" name="Dashboard Updated with New Results">
      <documentation>Platform has processed results and dashboard is ready (end of Phase 4)</documentation>
    </startEvent>

    <!-- Task 5.1: Notify Customer -->
    <task id="Task_NotifyCustomer" name="Notify Customer of New Results">
      <documentation>
        Send notification that new results are available.

        Notification includes:
        - Summary of new data
        - Link to dashboard
        - Any alerts/anomalies highlighted
        - Next sampling reminder (if applicable)

        Input: Dashboard updated
        Output: Email/notification sent
        Responsible: Customer Success (automated)
        Tool: Notification system
      </documentation>
    </task>

    <!-- Task 5.2: Customer Reviews Dashboard -->
    <task id="Task_CustomerReview" name="Customer Reviews Dashboard">
      <documentation>
        Customer logs in and reviews their data.

        Dashboard provides:
        - Latest NA results by water body
        - Trend visualizations
        - Correlation insights
        - Alerts and anomalies
        - Historical comparison

        Input: Dashboard access
        Output: Customer informed
        Responsible: Customer
        Tool: Platform web app
      </documentation>
    </task>

    <!-- Gateway: Support Needed? -->
    <exclusiveGateway id="Gateway_SupportNeeded" name="Interpretation Support Needed?">
      <documentation>Decision: Does the customer request interpretation support?</documentation>
    </exclusiveGateway>

    <!-- Task 5.3: Provide Interpretation Support -->
    <task id="Task_InterpretationSupport" name="Provide Interpretation Support">
      <documentation>
        Answer customer questions about their data.

        Support activities:
        - Explain NA trends
        - Interpret correlation findings
        - Discuss operational implications
        - Recommend actions based on data

        Input: Customer questions
        Output: Answers / insights provided
        Responsible: Customer Success
        Tool: Video call / email
      </documentation>
    </task>

    <!-- Gateway: Report Due? -->
    <exclusiveGateway id="Gateway_ReportDue" name="Periodic Report Due?">
      <documentation>Decision: Is it time for a scheduled periodic report?</documentation>
    </exclusiveGateway>

    <!-- Task 5.4: Generate Summary Report -->
    <task id="Task_GenerateSummaryReport" name="Generate Periodic Summary Report">
      <documentation>
        Create summary report for the reporting period.

        Report types:
        - Weekly summary (dashboard view / PDF)
        - Monthly review report (PDF + call)
        - Pilot completion report (PDF + presentation)

        Input: Accumulated data for period
        Output: Summary report
        Responsible: Customer Success
        Tool: Reporting module
      </documentation>
    </task>

    <!-- Gateway: Regulatory Report Needed? -->
    <exclusiveGateway id="Gateway_RegulatoryReport" name="Regulatory Report Needed?">
      <documentation>Decision: Is a regulatory or stakeholder report required?</documentation>
    </exclusiveGateway>

    <!-- Task: Generate AER Report -->
    <task id="Task_GenerateAERReport" name="Generate AER Compliance Report">
      <documentation>
        Create Alberta Energy Regulator compliance report.

        Report includes:
        - NA concentration trends
        - Sampling frequency compliance
        - Chain of custody documentation
        - QC/QA records
        - Exceedance notifications
        - Annual summary statistics

        Auto-generated PDF with:
        - Executive summary
        - Data tables in AER format
        - Trend charts
        - QC documentation
        - Digital signature/timestamp

        Input: Platform data
        Output: AER-ready PDF
        Responsible: Platform (automated) + Customer Success
        Tool: Reporting module
      </documentation>
    </task>

    <!-- Task: Generate Indigenous Community Report -->
    <task id="Task_GenerateCommunityReport" name="Generate Indigenous Community Report">
      <documentation>
        Create accessible report for Indigenous community stakeholders.

        Report design:
        - Plain language, no jargon
        - Visual infographics
        - Color-coded trends (green/yellow/red)
        - Historical comparison
        - Seasonal patterns (calendar view)
        - Site map with sampling locations

        Input: Platform data
        Output: Visual summary PDF
        Responsible: Customer Success
        Tool: Reporting module
      </documentation>
    </task>

    <!-- Task 5.5: Conduct Monthly Review Call -->
    <task id="Task_MonthlyReviewCall" name="Conduct Monthly Review Call">
      <documentation>
        Scheduled monthly call with customer.

        Agenda:
        - Review month's data and trends
        - Discuss any anomalies or concerns
        - Gather customer feedback
        - Identify action items
        - Plan any strategy adjustments

        Input: Monthly data summary
        Output: Action items, feedback documented
        Responsible: Customer Success
        Tool: Video call
      </documentation>
    </task>

    <!-- Gateway: Strategy Adjustment Needed? -->
    <exclusiveGateway id="Gateway_StrategyAdjust" name="Strategy Adjustment Needed?">
      <documentation>Decision: Based on learnings, should sampling strategy be adjusted?</documentation>
    </exclusiveGateway>

    <!-- Task 5.6: Adjust Sampling Strategy -->
    <task id="Task_AdjustStrategy" name="Adjust Sampling Strategy">
      <documentation>
        Modify sampling approach based on pilot learnings.

        Adjustments may include:
        - Change sampling frequency
        - Add/remove sampling locations
        - Adjust thresholds
        - Modify alert criteria

        Input: Pilot learnings, customer feedback
        Output: Updated sampling plan
        Responsible: Customer Success + Customer
        Tool: Collaborative decision
      </documentation>
    </task>

    <!-- Task: Log Customer Interaction -->
    <task id="Task_LogInteraction" name="Log Customer Interaction">
      <documentation>
        Document all customer interactions.

        Record:
        - Date and type of interaction
        - Topics discussed
        - Customer feedback
        - Action items
        - Follow-up required

        Input: Interaction details
        Output: CRM record updated
        Responsible: Customer Success
        Tool: CRM system
      </documentation>
    </task>

    <!-- End Event -->
    <endEvent id="Event_End" name="Customer Delivery Cycle Complete">
      <documentation>Customer has received results and any required reports. Ready for next sample batch.</documentation>
    </endEvent>

    <!-- Sequence Flows -->
    <sequenceFlow id="Flow_1" sourceRef="Event_Start" targetRef="Task_NotifyCustomer"/>
    <sequenceFlow id="Flow_2" sourceRef="Task_NotifyCustomer" targetRef="Task_CustomerReview"/>
    <sequenceFlow id="Flow_3" sourceRef="Task_CustomerReview" targetRef="Gateway_SupportNeeded"/>
    <sequenceFlow id="Flow_4" name="Yes" sourceRef="Gateway_SupportNeeded" targetRef="Task_InterpretationSupport"/>
    <sequenceFlow id="Flow_5" name="No" sourceRef="Gateway_SupportNeeded" targetRef="Gateway_ReportDue"/>
    <sequenceFlow id="Flow_6" sourceRef="Task_InterpretationSupport" targetRef="Gateway_ReportDue"/>
    <sequenceFlow id="Flow_7" name="Yes" sourceRef="Gateway_ReportDue" targetRef="Task_GenerateSummaryReport"/>
    <sequenceFlow id="Flow_8" name="No" sourceRef="Gateway_ReportDue" targetRef="Task_LogInteraction"/>
    <sequenceFlow id="Flow_9" sourceRef="Task_GenerateSummaryReport" targetRef="Gateway_RegulatoryReport"/>
    <sequenceFlow id="Flow_10" name="AER Required" sourceRef="Gateway_RegulatoryReport" targetRef="Task_GenerateAERReport"/>
    <sequenceFlow id="Flow_11" name="Community Required" sourceRef="Gateway_RegulatoryReport" targetRef="Task_GenerateCommunityReport"/>
    <sequenceFlow id="Flow_12" name="None Required" sourceRef="Gateway_RegulatoryReport" targetRef="Task_MonthlyReviewCall"/>
    <sequenceFlow id="Flow_13" sourceRef="Task_GenerateAERReport" targetRef="Task_MonthlyReviewCall"/>
    <sequenceFlow id="Flow_14" sourceRef="Task_GenerateCommunityReport" targetRef="Task_MonthlyReviewCall"/>
    <sequenceFlow id="Flow_15" sourceRef="Task_MonthlyReviewCall" targetRef="Gateway_StrategyAdjust"/>
    <sequenceFlow id="Flow_16" name="Yes" sourceRef="Gateway_StrategyAdjust" targetRef="Task_AdjustStrategy"/>
    <sequenceFlow id="Flow_17" name="No" sourceRef="Gateway_StrategyAdjust" targetRef="Task_LogInteraction"/>
    <sequenceFlow id="Flow_18" sourceRef="Task_AdjustStrategy" targetRef="Task_LogInteraction"/>
    <sequenceFlow id="Flow_19" sourceRef="Task_LogInteraction" targetRef="Event_End"/>
  </process>

